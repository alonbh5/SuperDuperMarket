<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>


<ScrollPane prefHeight="427.0" prefWidth="822.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="course.java.sdm.gui.CreateOrderMenu.StoreSumUpController">
   <content>
      <VBox alignment="CENTER" fillWidth="false" scaleShape="false">
         <children>
            <TableView fx:id="MainTable">
              <columns>
                <TableColumn fx:id="StoreIdColumn" prefWidth="75.0" text="Store ID" />
                <TableColumn fx:id="StoreNameColumn" prefWidth="95.0" text="Store Name" />
                  <TableColumn fx:id="PPKColumn" prefWidth="72.0" text="PPK" />
                  <TableColumn fx:id="DistanceColumn" prefWidth="118.0" text="Distance" />
                  <TableColumn fx:id="LocationColumn" prefWidth="95.0" text="Location" />
                  <TableColumn fx:id="ShippingColumn" prefWidth="95.0" text="Shipping Price" />
                  <TableColumn fx:id="AmountColumn" prefWidth="134.0" text="Amount Of Items" />
                  <TableColumn fx:id="ItemPriceColumn" prefWidth="134.0" text="Items Price" />
              </columns>
            </TableView>
            <Button fx:id="DiscountsButton" mnemonicParsing="false" onAction="#onDiscountAction" styleClass="MenuButtons" text="Show Discounts" />
         </children>
      </VBox>
   </content>
</ScrollPane>
