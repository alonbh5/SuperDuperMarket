<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="course.java.sdm.gui.OrderMenu.OrderMenuTileController">
   <children>
      <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="70.0" prefWidth="1081.0" spacing="20.0">
         <children>
            <Label text="Order ID:" />
            <Label fx:id="OrderIdLabel" />
            <Label text="Date:" />
            <Label fx:id="DateLabel" />
            <Label fx:id="UserNameLabel" text="User Name:" />
            <Label fx:id="UserNameLabel" />
            <Label text="User Loaction:" />
            <Label fx:id="LocationLabel" />
            <Label text="Order Type:" />
            <Label fx:id="OrderTypeLabel" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <TitledPane fx:id="ItemTile" text="Items">
        <content>
          <AnchorPane fx:id="ItemsPane" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
        </content>
      </TitledPane>
      <TitledPane fx:id="StoreTile" text="Store(s)">
        <content>
          <AnchorPane fx:id="StoresPane" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
        </content>
      </TitledPane>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
         <children>
            <Label text="Price For Items:" />
            <Label fx:id="ItemPriceLabel" />
            <Label text="Price For Shipping:" />
            <Label fx:id="ShippingPriceLabel" />
            <Label text="Total Price:" />
            <Label fx:id="TotalPriceLabel" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</VBox>
